<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Twig</title>
</head>
<body>
<h3>num: {{ num }}</h3>
<h3>{{ obj.x }}</h3>
<h3>{{ obj.foo }}</h3>
<ul>
    {% for el in arr %}
        <li>{{ el }}</li>
    {% else %}
        <li>Empty</li>
    {% endfor %}
</ul>
<ul>{{ arr|join(',') }}</ul>
</body>
</html>
